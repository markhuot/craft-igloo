{
    "name": "markhuot/craft-igloo",
    "type": "craft-plugin",
    "require": {
        "craftcms/cms": "^4.2",
        "hashids/hashids": "^4.1"
    },
    "require-dev": {
        "craftcms/craft": "^2.0",
        "markhuot/craft-pest": "@dev",
        "yiisoft/yii2-shell": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Huot",
            "email": "mark@markhuot.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "markhuot\\igloo\\": "src/"
        }
    },
    "extra": {
        "name": "Igloo",
        "handle": "igloo",
        "class": "markhuot\\igloo\\Igloo"
    }
}
